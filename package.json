    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "next": "^12.0.3",
    "prom-client": "^14.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swagger-ui-react": "^4.1.2",
    "swr": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
